      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "enumeration",
      "enum": [
        "online",
        "offline"
      ],
      "default": "offline",
      "required": true
    },
    "registerationEndDate": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "date",
      "required": true
